{
  "source_bucket": "baidu-download-new",
  "source_prefix": "UE4场景",
  "target_bucket": "world-data",
  "target_prefix": "raw",
  "target_content_folder": "Content",
  "fallback_markers": [
    "Blueprints",
    "Maps",
    "Map",
    "Level",
    "Levels"
  ],
  "exclude_patterns": [
    "manifest",
    "*.url",
    "*.txt",
    "*.md",
    "README",
    "License",
    "Docs",
    "Documentation"
  ],
  "prefer_duplicated_scene_name": true,
  "skip_existing": true,
  "description": "BOS场景复制配置 - 从源bucket复制UE场景到目标bucket。注意：bcecmd默认只列出几个对象，使用-a参数列出所有对象"
}
