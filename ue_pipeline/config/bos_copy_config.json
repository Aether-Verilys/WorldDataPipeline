{
  "source_bucket": "baidu-download-new",
  "source_prefix": "cdy-video-data/UnrealAssets",
  "target_bucket": "world-data",
  "target_prefix": "raw",
  "target_content_folder": "Content",
  "scenes": ["CHERRY_TREE"],
  "fallback_markers": [
    "Blueprints",
    "Maps",
    "Map",
    "Level",
    "Levels"
  ],
  "exclude_patterns": [
    "manifest",
    "*.url",
    "*.txt",
    "*.md",
    "README",
    "License",
    "Docs",
    "Documentation"
  ],
  "prefer_duplicated_scene_name": true,
  "skip_existing": true,
  "description": "BOS场景配置 - scenes字段可指定要下载的场景列表，为空则列出所有可用场景"
}
